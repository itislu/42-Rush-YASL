#!./yasl_linux_x64

{ = ! } move

{ "\n" + print } print_endl

{
	"Error: Invalid input!" print_endl
	1 exit
} error

{
	{
		"0" %< move:2 !
		"9" %> move:2 !
		2:||
	} is_not_int

	0:3
	[#] %? (
		#(
			3:=		// current index
			%]#[	// current char
			is_not_int ? error
			!!
			3:(= 1 +):3
		)
	) : error
} check_number


!!
# 1 != ? error
check_number
eval

{
	= 1 > ? (
		= 1 -
		fact
		*
	)
	= 0 < ? error
} fact

= 0 == ? 1 : fact

print_endl
