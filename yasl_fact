#!./yasl_linux_x64

{ = ! } move

{ print "\n" print } print_endl

{
	print_endl
	1 exit
} error

{
	{
		"0" %< move:2 !
		"9" %> move:2 !
		2:||
	} is_not_int

	0:3
	[#] %? (
		#(
			3:=		// current index
			%]#[	// current char
			is_not_int ? ("Error: Not a postive integer!" error)
			!!
			3:(= 1 +):3
		)
	) : ("Error: No number given!" error)
} check_number


!!
# 1 != ? ("Error: Need exactly 1 parameter!" error)
check_number
eval

{
	= 1 > ? (
		= 1 -
		fact
		*
	)
	= 0 < ? ("Error: This factorial would overflow in YASL!" error)
} fact

= 0 == ? 1 : fact

print_endl
