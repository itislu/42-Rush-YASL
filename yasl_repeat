#!./yasl_linux_x64

{ print "\n" print } print_endl

{ = ! } move

{ = print } print_lossless

{
	"0" %< move:2 !
	"9" %> move:2 !
	2:||
} is_not_int

{
	"Error: Invalid input!" print_endl
	1 exit
} error

!!

// argc check
# 1 <= ? error

0:move:1

1:( is_not_int ? error ):1

1:eval:1

1:( 0 %<= ?error ! ):1

{
	1:=:0

	#print_lossless !
	"\n" print

	1:(1 +):1	// increment
} print_n_times

